.warp{
  position: fixed;
  top:0;
  left: 0;
  width:100%;
  height: 132rpx;
  z-index: 100;
}
.top{
  width:100%;
  display: flex;
  text-align: center;
  font-size: 40rpx;
  background: #dc3516;
  color: #fff;
  height: 132rpx;
  
  align-items: center;
}
.top .callback {
  width: 20%;
  height: 100%;
  line-height: 132rpx;
  margin-left: 10rpx;
  display: flex;
  align-items: center;
}
.top .callback image{
  width: 50rpx;
  height: 50rpx;
}
.backTitle{
   margin-left: 20rpx;
   height: 132rpx;
   line-height: 132rpx;
}
.top .title{
  height: 132rpx;
   line-height: 132rpx;
   width: 70%;
}
.hold{
  width: 100%;
  height:132rpx; 
}