.content{
  overflow: hidden;
  width: 100%;
}
.list{
  display: flex;
  flex-direction: column;
  width: 100%;
}
.list .item {
  width: 100%;
  height: 130rpx;
  border-bottom: 4rpx solid #ccc;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center
}
.list .one{
  justify-content: flex-start
}
.list .left{
  display: flex;
}
.list .left image,.list .one image {
  width: 60rpx;
  height: 60rpx;
  margin-right: 20rpx;
  margin-left: 40rpx;
}
.list .right image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 40rpx;
}
.componey{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  padding-left: 40rpx;
  background: #ccc
}