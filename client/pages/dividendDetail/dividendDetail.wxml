<top-bar  title='分润详情' url='/pages/dividend/dividend'/>
<view class='content'>
   <view class='title'>当前网点个数：{{data.length}}</view>
   <view class='list'>
       <view class='item head'>
           <view>姓名</view>
           <view>网点地址</view>
           <view>营业额</view>
       </view>
       <label wx:for='{{data}}' wx:key="{{index}}">
       <view class='item'>
           <view>{{item.name}}</view>
           <view>{{item.address}}</view>
           <view>{{item.networkTodayMoney}}</view>
       </view>
       </label>
    </view>
</view>
