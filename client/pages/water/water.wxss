.content{
  width: 100%;
  overflow: hidden;
}
.msg{
  display: flex;
  align-content: center;
  justify-content: space-around;
  height: 100rpx;
  border-bottom: 2rpx solid #ccc;
}
.msg view{
  flex: 1;
 height: 100rpx;
 line-height: 100rpx;
 text-align: center
}
.msg button{
  width: 20%;
  margin: 15rpx;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 34rpx;
  background: rgb(220,53,22);
  color: #fff
}
.list {
  width: 100%;
  display: flex;
  height: 100rpx;
  justify-content: space-between;
  border-bottom: 2rpx solid #ccc;
}
.list view{
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  padding-left: 40rpx;
}
.list .detail{
  width: 20%;
  margin: 15rpx;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 34rpx;
  background: rgb(220,53,22);
  color: #fff
}
.history{
  width: 100%;
  height: 100rpx;
  margin-top:20rpx;
}
.history-detail{
  margin-top:20rpx;
  width: 100%;
 
  display: flex;
  justify-content: space-around
}
.history-detail view {
  text-align: center;
   height: 100rpx;
  line-height: 100rpx;
  font-size: 32rpx;
}
.no-data{
  text-align: center;
   height: 100rpx;
  line-height: 100rpx;
  width: 100%
}