.content{
  width: 100%;
  overflow: hidden;
}
.list {
  width: 100%;
  padding-left: 30rpx;
  padding-bottom: 40rpx;
  border-bottom: 8rpx solid #ccc;
  font-size:34rpx;

}
.list .item {
  width: 100%;
  height: 90rpx;
  padding-bottom: 15rpx;
  display: flex;
  align-items: flex-end;
  border-bottom: 2rpx solid #ccc;
}

.upload{
  width: 100%;
}
.upload-title{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  padding-left: 20rpx;
}
.upload-content{
  display: flex;
  width: 100%;
  flex-direction: column;
}
.upload-content .item{
  display: flex;
  flex-direction: column;
  flex: 1;
  align-items: center;
}
.upload-content .item image{
  width: 30%;
  height: 150rpx;
}
.upload-content .item .image{
  width: 80%;
  height: 350rpx;
}
.upload-content .item view{
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
}
button{
  background: #dc3516;
  color: #fff;
  margin-bottom: 10rpx;
}
.padding{
  padding:0 10rpx;
}
.xinhao{
  font-size:50rpx;
padding-right:8rpx;
color:red;
}
.paddingleft{
  padding-left: 22rpx;
}